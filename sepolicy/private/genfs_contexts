# Smart Charging
genfscon sysfs /sys/class/power_supply/battery/input_suspend           u:object_r:vendor_sysfs_battery_supply:s0

genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display/hbm            u:object_r:sysfs_amoled_feature:s0
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display/cabc           u:object_r:sysfs_amoled_feature:s0
